<project>
	<target name="preSetup" />
	<target name="postSetup" />
	<target name="preFetch" />
	<target name="postFetch" />
	<target name="getMapFiles" />
	<target name="preGenerate" />
	<target name="postGenerate" />
	<target name="preProcess" />
	<target name="postProcess" />
	<target name="preAssemble" />
	<target name="postAssemble" />
	<target name="allElements" />
	<target name="postBuild">
	    <property name="UpdateSiteStagingLocation" value="../edu.toronto.cs.ome.eclipse.update"/>
		<property name="sitePackagePrefix" value="edu.toronto.cs.ome.eclipse"/>
	    <property name="genericTargets" value="${builder}/customTargets.xml"/>
	    <property name="target" value="updateSiteExport"/>
	    <mkdir dir="${UpdateSiteStagingLocation}"/>
	    <mkdir dir="${UpdateSiteStagingLocation}/features"/>
	    <mkdir dir="${UpdateSiteStagingLocation}/plugins"/>
	    <ant antfile="build.xml" dir="../edu.toronto.cs.ome.eclipse.feature" target="build.update.jar" inheritAll="true">
		    <property name="feature.destination" value="${UpdateSiteStagingLocation}/features"/>
		    <property name="plugin.destination" value="${UpdateSiteStagingLocation}/plugins"/>
		</ant>
	</target>
</project>