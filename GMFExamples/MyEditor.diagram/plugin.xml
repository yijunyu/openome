         <includes>
            <contentExtension pattern="MyEditor.diagram.resourceContent"/>
			<contentExtension pattern="MyEditor.diagram.domainResourceContent"/>
            <contentExtension pattern="MyEditor.diagram.navigatorLinkHelper"/>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding viewerId="org.eclipse.ui.navigator.ProjectExplorer">
