   <extension point="org.eclipse.ui.navigator.navigatorContent">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="MyEditor.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="MyEditor.diagram.navigator.MyeditorNavigatorContentProvider" 
            labelProvider="MyEditor.diagram.navigator.MyeditorNavigatorLabelProvider"
            icon="icons/obj16/MyEditorDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="myeditor_diagram"/>
               </and>
               <instanceof value="MyEditor.diagram.navigator.MyeditorAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="MyEditor.diagram.navigator.MyeditorAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="MyEditor.diagram.navigatorSorter" 
               class="MyEditor.diagram.navigator.MyeditorNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="myeditor_diagram"/>
                  </and>
                  <instanceof value="MyEditor.diagram.navigator.MyeditorAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="MyEditor.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="MyEditor.diagram.navigator.MyeditorDomainNavigatorContentProvider" 
            labelProvider="MyEditor.diagram.navigator.MyeditorDomainNavigatorLabelProvider"
            icon="icons/obj16/MyEditorDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="myeditor"/>
               </and>
               <and>
                  <instanceof value="org.eclipse.emf.ecore.EObject"/>
                  <test property="MyEditor.diagram.isDomainModelElement"/>
               </and>
            </or>
         </triggerPoints>
         <possibleChildren>
            <and>
       	       <instanceof value="org.eclipse.emf.ecore.EObject"/>
       	       <test property="MyEditor.diagram.isDomainModelElement"/>
       	    </and>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="MyEditor.diagram.navigatorActionProvider"
            class="MyEditor.diagram.navigator.MyeditorNavigatorActionProvider">
         <enablement>
            <or>
               <instanceof value="MyEditor.diagram.navigator.MyeditorAbstractNavigatorItem"/>
            </or>
         </enablement>
      </actionProvider>
   </extension>
   
	   <extension point="org.eclipse.ui.navigator.linkHelper">
      <?gmfgen generated="true"?>
      <linkHelper
            id="MyEditor.diagram.navigatorLinkHelper"
            class="MyEditor.diagram.navigator.MyeditorNavigatorLinkHelper">
         <editorInputEnablement>
            <and>
               <instanceof value="org.eclipse.emf.common.ui.URIEditorInput"/>
               <test property="MyEditor.diagram.isURIEditorInput"/>
            </and>
         </editorInputEnablement>
         <selectionEnablement>
            <instanceof value="MyEditor.diagram.navigator.MyeditorAbstractNavigatorItem"/>
         </selectionEnablement>
      </linkHelper>
   </extension>
