#!/bin/bash
VERSION=3.3.1.1
IDE=/IDE
export ECLIPSE_HOME=$IDE/sdk/$VERSION/eclipse
rm -rf configuration
mkdir configuration
cp $ECLIPSE_HOME/configuration/config.ini configuration
rm -rf $ECLIPSE_HOME/links
mkdir $ECLIPSE_HOME/links
cp ../links2/*.* $ECLIPSE_HOME/links
$ECLIPSE_HOME/eclipse -configuration configuration -nosplash -application org.eclipse.ant.core.antRunner -buildfile build.xml $*
