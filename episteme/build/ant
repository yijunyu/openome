#!/bin/bash
V=3.3.1.1
export ECLIPSE_SDK=$HOME/IDE/sdk/$V/eclipse
rm -rf configuration
mkdir configuration
cp $ECLIPSE_SDK/configuration/config.ini configuration
rm -rf $ECLIPSE_SDK/links
mkdir $ECLIPSE_SDK/links
cp ../links/*.* $ECLIPSE_SDK/links
$ECLIPSE_SDK/eclipse -configuration configuration -nosplash -application org.eclipse.ant.core.antRunner -buildfile build.xml $*
