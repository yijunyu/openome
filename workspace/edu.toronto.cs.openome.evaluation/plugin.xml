<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   
     <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="edu.toronto.cs.openome.evaluation.handlers.InteractiveQualReasonerHandler"
            id="openome_IntQualReasoning_cmd"
            name="openome_IntQualReasoning_cmd">
      </command>
      </extension>

   <extension 
		id="openome_reasoning_menus"
		point="org.eclipse.ui.menus">
	
		<menuContribution 
			locationURI="menu:openome_top_menu?after=additions">
			
   			<menu
         		id="openome_reasoning_main_menu"
         		label="Reasoning">
         		<command 
					commandId="openome_IntQualReasoning_cmd"
					icon="icons/sample.gif"
					id="openome_IntQualReasoning_menu"
					label="Interactive Qualitative Reasoning"
					tooltip=""
					mnemonic="Q">
				</command>
   			</menu>
		</menuContribution>
	</extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="edu.toronto.cs.openome.evaluation.windows.evaluationPopUpWindow"
            id="edu.toronto.cs.openome.evaluation.windows.evaluationPopUpWindow"
            name="User Evaluation Input">
      </page>
   </extension>

</plugin>
