<?xml version="1.0" encoding="ASCII"?>
<xmi:XMI xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:edu.toronto.cs.goalmodel="http:///edu/toronto/cs/goalmodel.ecore">
  <edu.toronto.cs.goalmodel:actor name="Aspect Minimize Risk">
    <goals name="Minimize Risk" type="LEAF" label="PartiallyDenied" exclusive="false" sequential="false" mode="SOFT"/>
  </edu.toronto.cs.goalmodel:actor>
  <edu.toronto.cs.goalmodel:actor name="Aspect Customer Satisfaction">
    <goals name="Customer Satisfaction" type="LEAF" label="PartiallySatisfied" exclusive="false" sequential="false" mode="SOFT"/>
  </edu.toronto.cs.goalmodel:actor>
  <edu.toronto.cs.goalmodel:goal name="Sell to Customer" type="AND" exclusive="false" mode="TASK">
    <goal name="Get Customer Order" exclusive="false" sequential="false">
      <goal name="Get Phone Order" type="LEAF" label="Unknown" exclusive="false" sequential="false" mode="TASK"/>
      <goal name="Get Fax Order" type="LEAF" label="Unknown" exclusive="false" sequential="false" mode="TASK"/>
      <goal name="Get Online Order" type="LEAF" exclusive="false" sequential="false" mode="TASK"/>
    </goal>
    <goal name="Process Customer Order" exclusive="false" sequential="false">
      <goal name="Bill, Build, then Ship" type="AND" label="Unknown" exclusive="false" mode="TASK">
        <goal name="Bill" type="LEAF" label="Unknown" exclusive="false" sequential="false" mode="TASK"/>
        <goal name="Collect Payment" type="LEAF" label="Unknown" exclusive="false" sequential="false" mode="TASK"/>
        <rule type="HURT" target="/1/@goals.0"/>
        <rule type="MAKE" target="/0/@goals.0"/>
      </goal>
      <goal name="Build, then Ship and Bill" type="AND" exclusive="false" mode="TASK">
        <goal name="Build and Package Order" type="LEAF" exclusive="false" sequential="false" mode="TASK"/>
        <goal name="Apply Process To Customer" exclusive="false" sequential="false">
          <goal name="Apply to Any Customer" type="AND" exclusive="false" sequential="false" mode="TASK">
            <rule type="HURT" target="/0/@goals.0"/>
          </goal>
          <goal name="Apply to Trusted Customer" type="AND" label="Unknown" exclusive="false" mode="TASK">
            <goal name="Approve Order" type="LEAF" label="Unknown" exclusive="false" sequential="false" mode="TASK"/>
            <goal name="Determine Trustworthiness of Customer" label="Unknown" exclusive="false" sequential="false">
              <goal name="Check if Return Customer" type="LEAF" label="Unknown" exclusive="false" sequential="false" mode="TASK"/>
              <goal name="Check Credit Rating" type="LEAF" label="Unknown" exclusive="false" sequential="false" mode="TASK"/>
            </goal>
            <rule target="/0/@goals.0"/>
          </goal>
        </goal>
        <goal name="Ship and Bill" type="AND" exclusive="false" sequential="false" parallel="true" mode="TASK">
          <goal name="Ship Order" type="LEAF" exclusive="false" sequential="false" mode="TASK"/>
          <goal name="Bill and Collect Payment" type="LEAF" exclusive="false" sequential="false" mode="TASK"/>
        </goal>
        <rule target="/1/@goals.0"/>
      </goal>
    </goal>
  </edu.toronto.cs.goalmodel:goal>
</xmi:XMI>
