<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="q7.dsl.editor.Q7dslEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="q7"
            icon="icons/file.gif"
            id="q7.dsl.editor.Q7dslEditor"
            name="q7dsl Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="q7.dsl.wizards.NewQ7dslProjectWizard"
			icon="icons/sample.gif"
			id="q7.dsl.wizards.NewQ7dslProjectWizard"
			name="q7dsl Project"
			project="true">
		</wizard>
         
		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="q7.dsl.wizards.NewQ7dslFileWizard"
			icon="icons/file.gif"
			id="q7.dsl.wizards.NewQ7dslFileWizard"
			name="q7dsl Editor file">
		</wizard>
	</extension>
</plugin>
