<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension point="core.convertor">
      <convertor id="Q7 to Goal Model" 
      		class="edu.toronto.cs.openome.conversion.convertor.Q7_GoalModel"
            input="q7" output="oom"/>
   </extension>
  <extension point="org.eclipse.ui.popupMenus">
	<objectContribution
		objectClass="org.eclipse.core.resources.IFile"
		nameFilter="*.q7"
		id="q7">
		<menu label="Convert the Q7 representation into" path="additions" id="menu1">
			<separator name="group1"> </separator>
        </menu>
        <action
               class="edu.toronto.cs.openome.conversion.action.Q7_GoalModel"
               icon="icons/goal.gif"
               id="GoalModelAction"
               label="OpenOME Model"
               menubarPath="menu1/group1"
               tooltip="openOME model">
		</action>         
	</objectContribution>

   </extension>
  <extension
        point="org.eclipse.ui.exportWizards">
     <wizard
           category="org.bflow.toolbox.export.bflow"
           class="edu.toronto.cs.openome.conversion.convertor.ExportWizard"
           icon="icons/telos.gif"
           id="edu.toronto.cs.openome.conversion.convertor.ExportWizardID"
           name="Q7 Conversion">
        <description>
           Export the diagram to Q7 file
        </description>
     </wizard>
  </extension>

</plugin>
