<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.cs.toronto.edu/~yijun/requirements"  xmlns:req="http://www.cs.toronto.edu/~yijun/requirements">
 <goal name=""May Security" [system]">
  <rule op="AND"/>
  <goal name=""May Confidentiality" [system]" />
  <goal name=""May Info. flow Security" [system]" />
</goal>
 <goal name=""May Usability" [ui]">
  <rule op="AND"/>
  <goal name=""May Usability" [gui]">
   <rule op="AND"/>
   <goal name=""May Usability" [font]" />
   <goal name=""May Usability" [layout]" />
</goal>
  <goal name=""May Usability" [language]" />
</goal>
 <goal name="Front [shop]">
  <rule op="AND"/>
  <goal name="Informing [shop]">
   <rule op="AND"/>
   <goal name="Reporting [shop]">
    <rule op="AND"/>
    <goal name=""Page Layout" [gui]">
     <rule op="MAKE" to=""May Usability" [layout]" />
    </goal>
    <goal name="Customization [language]">
     <rule op="OR"/>
     <goal name="Customizing [English]">
      <rule op="MAKE" to=""May Usability" [language]" />
     </goal>
</goal>
    <goal name="Infobox [font]">
     <rule op="MAKE" to=""May Usability" [font]" />
    </goal>
    <goal name="Reporting [account]" />
    <goal name="Reporting [product]" />
    <goal name="Reporting [stock]" />
    <goal name="Transaction [account]">
     <rule op="OR"/>
     <goal name=""Database Table" [transaction]">
      <rule op="MAKE" to=""May Integrity" [data]" />
     </goal>
</goal>
    <goal name="Transaction [product]">
     <rule op="OR"/>
     <goal name=""Database Table" [transaction]">
      <rule op="MAKE" to=""May Integrity" [data]" />
     </goal>
</goal>
    <goal name="Transaction [stock]">
     <rule op="OR"/>
     <goal name=""Database Table" [transaction]">
      <rule op="MAKE" to=""May Integrity" [data]" />
     </goal>
</goal>
</goal>
   <goal name="Searching [shop]">
    <rule op="AND"/>
    <goal name=""Page Layout" [gui]">
     <rule op="MAKE" to=""May Usability" [layout]" />
    </goal>
    <goal name="Customization [language]">
     <rule op="OR"/>
     <goal name="Customizing [English]">
      <rule op="MAKE" to=""May Usability" [language]" />
     </goal>
</goal>
    <goal name="Infobox [font]">
     <rule op="MAKE" to=""May Usability" [font]" />
    </goal>
    <goal name="Navigating [stock]" />
    <goal name="Searching [stock]" />
    <goal name="Transaction [stock]">
     <rule op="OR"/>
     <goal name=""Database Table" [transaction]">
      <rule op="MAKE" to=""May Integrity" [data]" />
     </goal>
</goal>
</goal>
</goal>
  <goal name="Managing [shop]">
   <rule op="AND"/>
   <goal name=""Page Layout" [gui]">
    <rule op="MAKE" to=""May Usability" [layout]" />
   </goal>
   <goal name="Customization [language]">
    <rule op="OR"/>
    <goal name="Customizing [English]">
     <rule op="MAKE" to=""May Usability" [language]" />
    </goal>
</goal>
   <goal name="Infobox [font]">
    <rule op="MAKE" to=""May Usability" [font]" />
   </goal>
   <goal name="Managing [account]" />
   <goal name="Managing [product]" />
   <goal name="Managing [stock]" />
   <goal name="SSL [protocol]">
    <rule op="MAKE" to=""May Info. flow Security" [system]" />
   </goal>
   <goal name="Transaction [account]">
    <rule op="OR"/>
    <goal name=""Database Table" [transaction]">
     <rule op="MAKE" to=""May Integrity" [data]" />
    </goal>
</goal>
   <goal name="Transaction [product]">
    <rule op="OR"/>
    <goal name=""Database Table" [transaction]">
     <rule op="MAKE" to=""May Integrity" [data]" />
    </goal>
</goal>
   <goal name="Transaction [stock]">
    <rule op="OR"/>
    <goal name=""Database Table" [transaction]">
     <rule op="MAKE" to=""May Integrity" [data]" />
    </goal>
</goal>
</goal>
  <goal name="Shopping [shop]">
   <rule op="AND"/>
   <goal name=""Checking Out" [cart,product,account,stock]">
    <rule op="AND"/>
    <goal name=""Login/Logout" [account]" />
    <goal name=""Page Layout" [gui]">
     <rule op="MAKE" to=""May Usability" [layout]" />
    </goal>
    <goal name=""Password Protection" [account]">
     <rule op="MAKE" to=""May Confidentiality" [system]" />
    </goal>
    <goal name="Clearing [cart]" />
    <goal name="Customization [language]">
     <rule op="OR"/>
     <goal name="Customizing [English]">
      <rule op="MAKE" to=""May Usability" [language]" />
     </goal>
</goal>
    <goal name="Infobox [font]">
     <rule op="MAKE" to=""May Usability" [font]" />
    </goal>
    <goal name="SSL [protocol]">
     <rule op="MAKE" to=""May Info. flow Security" [system]" />
    </goal>
    <goal name="Transaction [account]">
     <rule op="OR"/>
     <goal name=""Database Table" [transaction]">
      <rule op="MAKE" to=""May Integrity" [data]" />
     </goal>
</goal>
    <goal name="Transaction [cart]">
     <rule op="OR"/>
     <goal name=""Session Cookie" [transaction]">
      <rule op="MAKE" to=""May Responsiveness" [transaction]" />
     </goal>
</goal>
    <goal name="Transaction [product]">
     <rule op="OR"/>
     <goal name=""Database Table" [transaction]">
      <rule op="MAKE" to=""May Integrity" [data]" />
     </goal>
</goal>
    <goal name="Transaction [stock]">
     <rule op="OR"/>
     <goal name=""Database Table" [transaction]">
      <rule op="MAKE" to=""May Integrity" [data]" />
     </goal>
</goal>
    <goal name="Updating [account,product]" />
    <goal name="Updating [product,stock]" />
</goal>
   <goal name="Getting [account]" />
   <goal name="Preparing [cart,product]">
    <rule op="AND"/>
    <goal name=""Page Layout" [gui]">
     <rule op="MAKE" to=""May Usability" [layout]" />
    </goal>
    <goal name="Adding [item,cart]" />
    <goal name="Customization [language]">
     <rule op="OR"/>
     <goal name="Customizing [English]">
      <rule op="MAKE" to=""May Usability" [language]" />
     </goal>
</goal>
    <goal name="Infobox [font]">
     <rule op="MAKE" to=""May Usability" [font]" />
    </goal>
    <goal name="Selecting [item,cart]" />
    <goal name="Transaction [cart]">
     <rule op="OR"/>
     <goal name=""Session Cookie" [transaction]">
      <rule op="MAKE" to=""May Responsiveness" [transaction]" />
     </goal>
</goal>
</goal>
</goal>
</goal>
</model>
